<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <EditTextPreference
        android:defaultValue="@string/pref_default_username"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/pref_title_username"
        android:key="email" />
    <EditTextPreference
        android:defaultValue="@string/pref_default_password"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/pref_title_password"
        android:key="password"
        android:password="true" />
    <EditTextPreference
        android:defaultValue="@string/pref_default_url"
        android:selectAllOnFocus="true"
        android:singleLine="true"
        android:title="@string/pref_title_url"
        android:key="urlschedule" />
    <!--<Preference>
        <intent android:targetClass="com.m2dl.mobe.miniprojetandroid.configuration.ProfileActivity"/>
    </Preference>-->

</PreferenceScreen>
